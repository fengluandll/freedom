<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:p="http://primefaces.org/ui"
      lang="es">
<h:head>
<script type="text/javascript">
	function redirecciona(){
		
		window.location = '/joappWeb/login.xhtml';
		
		}
</script>

<link rel="stylesheet" type="text/css" href="css/general.css" />
<link href="images/headset_icono.png" rel="shortcut icon" type='image/png'/>
<link rel="stylesheet" type="text/css" href="css/bootstrap.css"/>
<link rel="stylesheet" type="text/css" href="css/bootstrap-theme.css"/>
<link rel="stylesheet" type="text/css" href="css/custom.css"/>
<link rel="stylesheet" type="text/css" href="css/loginCss.css"/>
<script type="text/javascript" src="js/bootstrap.js"></script>
<script type="text/javascript" src="js/jquery.min.js"></script>
<script type="text/javascript" src="js/login.js"></script>

   <style type="text/css">
			
		   body{
		   		/*background-color:rgb(224,224,224)  ;*/
		   		/*background-image:url('images/background_black.jpg');*/
		   		 
		   }
   </style>
</h:head>
<h:body>
<span id="startPalabra" onclick="redireccionaWeb()" style="cursor: pointer;">
	start
</span>
<span class="text-danger" onclick="redireccionaWeb()" style="cursor: pointer;position: relative;
															top: 8px;
															right: px;
															left: 32px;
															font-size: 30pt;
															font-weight: 100;
															font-family: Helvetica, sans-serif">
	Web
</span>
<span id="linePalabra" onclick="redireccionaWeb()" style="cursor: pointer;">
	
</span>
<span id="web" onclick="redireccionaWeb()" style="cursor: pointer;">
	www.startonline.com.mx
</span>
<p:graphicImage value="images/Home2-Blue-32.png" 
				onclick="javascript:redirecciona()" 
				style="cursor:pointer;
						position: relative;
					    top: 0px;
					    right: 0px;
					    left: 10px;"/>
<h:form id="frmLogin">

<div style="position: relative;top:20px;left:320px;width: 20px;">


</div>
<div id="divPrincipal">  
<p:panelGrid styleClass="borderCero"  style="overflow:hidden;
											  border-radius: 10px 10px 10px 10px;
											  -moz-border-radius: 10px 10px 10px 10px;
											  -webkit-border-radius: 10px 10px 10px 10px;">
	<f:facet name="header">
	  
        <p:row>
           <p:column colspan="4" styleClass="etiqueta1">Se enviara a tu e-mail tu contrase√±a</p:column>
        </p:row>
    </f:facet>        
        <p:row>
        	<p:column>
    			<h:outputText value="Ingresa tu correo : " styleClass="etiqueta1"/>
    		</p:column>
          	<p:column>
    			<p:inputText value="#{loginBean.correo}" required="true" label="Correo"/>
    		</p:column>
        </p:row>
    <f:facet name="footer">
        <p:row>
    		<p:column colspan="2" style="text-align: center">
    			<p:commandButton value="Enviar" actionListener="#{loginBean.olvidoContrasena}" update="msgGral"/>
    		</p:column>
    		
        </p:row>
        
	</f:facet>
	
</p:panelGrid>
<p:messages id="msgGral" showDetail="true"/>  
</div>  
</h:form>
<p:ajaxStatus style="width:16px;height:16px;" id="ajaxStatusPanel">
    <f:facet name="start">
        <h:graphicImage value="../images/ajax-loader.gif" />
    </f:facet>
     
    <f:facet name="complete">
        <h:outputText value="" />
    </f:facet>
</p:ajaxStatus>
</h:body>
</html>