<p:layout fullPage="true" widgetVar="layoutWdgt">  
	  
<p:layoutUnit position="north" size="100" 
			  header="Bienvenid@: #{studentPersonalBean.givenNames}&#160;&#160;&#160;&#160;No Cta: #{studentPersonalBean.numCta}&#160;&#160;&#160;&#160;Rol: #{studentPersonalBean.rolName}" 
			  resizable="false" 
			  closable="false" 
			  collapsible="false" maxSize="100">
<h:form id="frm1">
	<div id="divHeader" style="height: 50px;border: black solid 0px ;">
	 <a id="logoStart" onclick="direcciona3();" style="cursor:pointer" >
	   <img src="../images/headset_icono.png"  id="startOnlineLogo"/>
	   <span id="startPalabraH">start</span>
	   <span id="onPalabraH">Web</span>
	   <!--<span id="linePalabraH">line</span>-->
	 </a>
<p:panelGrid style="position:absolute;left:30%;top:1%;" styleClass="borderCero2">
<p:row>
	<p:column>
	 <p:commandLink oncomplete="PF('dlg').show();" update=":frmdialog" title="Mis apuntes"> 
	 	<img id="posiNotpng" src="../images/edit-tomboy-icon.png"   />
 	     <span id="misNotasPalabra">Mis notas</span>
	 </p:commandLink>
	</p:column>
	<p:column>
	  <p:commandLink onclick="javascript:direcciona()" title="Mi perfil">
	 	  <img id="perfilPng" src="../images/user-icon.png"   />
 	      <span id="perfilPalabra">Mi perfil</span>
	 </p:commandLink>
	</p:column>	
	<p:column>
		<p:lightBox iframe="true" 
					id="id_ayuda3" onHide="true" widgetVar="wid_preg">
			 <h:outputLink value="/joappWeb/administracion/preguntaTema.xhtml">
			 	  <img id="ayudaPng" src="../images/pregunta.png"   />
		 	      <span id="ayudaPalabra">Pregunta</span>
			 </h:outputLink>
		</p:lightBox>	
 	</p:column>
 	<p:column>
	 <p:commandLink oncomplete="PF('dlgBuzon').show();" title="buzon de sugerencias">
	 	  <img id="buzonPng" src="../images/buzon_32.png"   />
 	      <span id="buzonPalabra">Buzón</span>
	 </p:commandLink>
	</p:column>
	<p:column>
	  <h:commandLink action="#{loginBean.salir}" title="Salir"> 
	 	<img id="salirPng" src="../images/exit2.png"   />
 	     <span id="salirPalabra">Salir</span>
	 </h:commandLink>
	 </p:column>
	</p:row>	 
</p:panelGrid>
	</div>
</h:form>	
</p:layoutUnit>

