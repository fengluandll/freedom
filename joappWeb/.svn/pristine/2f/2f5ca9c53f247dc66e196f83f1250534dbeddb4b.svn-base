  <managed-bean-class>mx.javaonline.beans.ArbolCursosBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>dashJavaBasicoBean</managed-bean-name>
  <managed-bean-class>mx.javaonline.dasboard.beans.DashJavaBasicoBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>topicsCarouselBean</managed-bean-name>
  <managed-bean-class>mx.javaonline.carousel.beans.TopicsCarouselBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>componentTreeBean</managed-bean-name>
  <managed-bean-class>mx.javaonline.util.ComponentTreeBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>openVideoBean</managed-bean-name>
  <managed-bean-class>mx.javaonline.carousel.beans.OpenVideoBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>ratingController</managed-bean-name>
  <managed-bean-class>mx.javaonline.carousel.beans.RatingController</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>cronometroTiempo</managed-bean-name>
  <managed-bean-class>mx.javaonline.beans.CronometroTiempo</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>timerContent</managed-bean-name>
  <managed-bean-class>mx.javaonline.beans.TimerContent</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>fileUploadController</managed-bean-name>
  <managed-bean-class>mx.javaonline.controlador.FileUploadController</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>fileDownloadController</managed-bean-name>
  <managed-bean-class>mx.javaonline.controlador.FileDownloadController</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>notasBeans</managed-bean-name>
  <managed-bean-class>mx.javaonline.notes.beans.NotasBeans</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>dialogNotesBean</managed-bean-name>
  <managed-bean-class>mx.javaonline.notes.beans.DialogNotesBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>nuevoUsuarioBean</managed-bean-name>
  <managed-bean-class>mx.javaonline.admin.beans.NuevoUsuarioBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>asignaCursosUsuario</managed-bean-name>
  <managed-bean-class>mx.javaonline.admin.beans.AsignaCursosUsuario</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>sendMails</managed-bean-name>
  <managed-bean-class>mx.javaonline.util.mail.SendMails</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>buscaActualizaUsuario</managed-bean-name>
  <managed-bean-class>mx.javaonline.admin.beans.BuscaActualizaUsuario</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>nuevosCursos</managed-bean-name>
  <managed-bean-class>mx.javaonline.admin.beans.NuevosCursos</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>statusBarTopicDAO</managed-bean-name>
  <managed-bean-class>mx.javaonline.daos.StatusBarTopicDAO</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>agregaContenidoBean</managed-bean-name>
  <managed-bean-class>mx.javaonline.admin.beans.AgregaContenidoBean</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>menuPerfil</managed-bean-name>
  <managed-bean-class>mx.javaonline.admin.beans.MenuPerfil</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>cambiaPasswordBean</managed-bean-name>
  <managed-bean-class>mx.javaonline.admin.beans.CambiaPasswordBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>actualizaDatosBean</managed-bean-name>
  <managed-bean-class>mx.javaonline.admin.beans.ActualizaDatosBean</managed-bean-class>
  <managed-bean-scope>view</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/login.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/home/home.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/home/home.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>salir</from-outcome>
   <to-view-id>/login.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/dashboards/dashBoardCourses.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>topicCarousel</from-outcome>
   <to-view-id>/carousel/topicCarousel.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/administracion/nuevoUsuario.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>success</from-outcome>
   <to-view-id>/administracion/asignaCursosUsuario.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/administracion/miPerfil.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>cambiaPass</from-outcome>
   <to-view-id>/administracion/cambiarContrasenia.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>updateData</from-outcome>
   <to-view-id>/administracion/actualizaDatos.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/administracion/cambiarContrasenia.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>regresar</from-outcome>
   <to-view-id>/administracion/miPerfil.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <application/>
</faces-config>
