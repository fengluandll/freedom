<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:p="http://primefaces.org/ui"
      xmlns:f="http://java.sun.com/jsf/core">
<h:head>
<link rel="stylesheet" type="text/css" href="../css/general.css"/>
<style type="text/css">

.ui-carousel {
		border: none;
}
		
			
</style>
<script type="text/javascript">

function atras(unitId){
	var lol = window.parent.document.getElementById('contenPrincipal');
	lol.src = '/joappWeb/carousel/topicCarousel.xhtml';
	
	}  
function start() {  
    PF('statusDialog').show();  
}  
  
function stop() {  
    PF('statusDialog').hide();  
}  

</script>
</h:head>
<p:dialog modal="true" widgetVar="statusDialog" header="Status" draggable="false" closable="false" resizable="false">  
    <p:graphicImage value="/images/ajaxloadingbar.gif" />  
</p:dialog> 
 
<h:form id="formVisualizaPractica">

<center>         
<p:panelGrid styleClass="borderCero3">
<p:row>
	<p:column>
		<p:panelGrid>
			<p:row>
				<p:column>
				<p:commandLink onclick="javascript:atras(#{sessionScope['unitId']});">
			   		<p:graphicImage url="/images/Arrow-Left-blue-48.png" 
			   						style="margin-top:-12px;margin-bottom:-7px;" 
			   						title="Regresar al dashBoard"/>
			   </p:commandLink>
			   </p:column>
			</p:row>
	   </p:panelGrid>
	</p:column>
</p:row>
</p:panelGrid>

<p:separator id="separator"/> 
<p:spacer height="20" />

<p:panel header="Descarga material">

	<p:panelGrid styleClass="borderCero3" style="margin: 0 auto;">
		<p:row>
			<p:column style="text-align:center;">
				<p:graphicImage value="/images/DescargaPDF.png"/>
			</p:column>
			<p:column rendered="#{!empty fileDownloadController.linkPractic2}" style="text-align:center;" colspan="2">
				<p:graphicImage value="/images/comoQuedaria.png"/>
			</p:column>
		</p:row>
		<p:row>
			<p:column>
				<p:commandButton id="downloadLink" value="Descargar" ajax="false" onclick="PrimeFaces.monitorDownload(start, stop)"   
				        icon="ui-icon-arrowthichk-s">  
				    <p:fileDownload value="#{fileDownloadController.getArchivo('practica')}" />  
				</p:commandButton>  
			</p:column>
			<p:column rendered="#{!empty fileDownloadController.linkPractic2}">
		 		<p:lightBox id="lighbox2" iframe="true">
				 
				  <h:outputLink value="#{fileDownloadController.linkPractic2}">
				  	<p:commandButton value="Como quedaría" />
				   </h:outputLink>
				</p:lightBox> 
				
			</p:column>
			<p:column rendered="#{!empty fileDownloadController.linkPractic3}">
		 		<p:lightBox id="lighbox3" iframe="true">
				   <h:outputLink value="#{fileDownloadController.linkPractic3}">
				  	<p:commandButton value="Como quedaría" />
				   </h:outputLink>
				</p:lightBox> 
				
			</p:column>
		</p:row>
	</p:panelGrid> 
</p:panel>
               
<p:spacer height="20" />
<p:separator id="separator2"/>
                   
</center>      					             
</h:form>
</html>