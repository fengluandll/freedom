<?xml version='1.0' encoding='UTF-8' ?>
<!DOCTYPE html>
<html xmlns="http://www.w3.org/1999/xhtml"
      xmlns:f="http://java.sun.com/jsf/core"
      xmlns:h="http://java.sun.com/jsf/html"
      xmlns:p="http://primefaces.org/ui"
      lang="es">
<h:head>
<script type="text/javascript">
	function redirecciona(){
		
		window.location = '/joappWeb/login.xhtml';
		
		}
</script>

<link rel="stylesheet" type="text/css" href="css/general.css" />
<link href="images/headset_icono.png" rel="shortcut icon" type='image/png'/>
<link rel="stylesheet" type="text/css" href="css/bootstrap.css"/>
<link rel="stylesheet" type="text/css" href="css/bootstrap-theme.css"/>
<link rel="stylesheet" type="text/css" href="css/custom.css"/>
<link rel="stylesheet" type="text/css" href="css/loginCss.css"/>
<script type="text/javascript" src="js/bootstrap.js"></script>
<script type="text/javascript" src="js/jquery.min.js"></script>
<script type="text/javascript" src="js/login.js"></script>

   <style type="text/css">
			
		   body{
		   		/*background-color:rgb(224,224,224)  ;*/
		   		/*background-image:url('images/background_black.jpg');*/
		   		 
		   }
   </style>
</h:head>
<h:body>
<span id="startPalabra" onclick="redireccionaWeb()" style="cursor: pointer;">
	start
</span>
<span class="text-danger" onclick="redireccionaWeb()" style="cursor: pointer;position: relative;
															top: 8px;
															right: px;
															left: 32px;
															font-size: 30pt;
															font-weight: 100;
															font-family: Helvetica, sans-serif">
	Web
</span>
<span id="linePalabra" onclick="redireccionaWeb()" style="cursor: pointer;">
	
</span>
<span id="web" onclick="redireccionaWeb()" style="cursor: pointer;">
	www.startonline.com.mx
</span>
<p:graphicImage value="images/Home2-Blue-32.png" 
				onclick="javascript:redirecciona()" 
				style="cursor:pointer;
						position: relative;
					    top: 0px;
					    right: 0px;
					    left: 40px;"/>
<h:form id="frmLogin">

<div id="grid" class="container" style="width: 25%;">
  
  <div class="row" id="pwd-container" style="">
   <div class="form-group" style=""></div>
       <section class="login-form">
       <!--  <img src="http://i.imgur.com/RcmcLv4.png" class="img-responsive" alt="" />  -->
       <h4 class="text-info" style="text-align: center;">Se enviara tu nueva contrase√±a a tu Email</h4>

         <p:inputText type="email"
         			  id="idUser" 
         			  placeholder="Email" 
         			  styleClass="form-control" 
         			  value="#{loginBean.correo}" 
         			  required="true" 
         			  label="Correo"
         			  style="position:relative;top:20px;" />
		 
		<p:commandButton value="Enviar"
						 action="#{loginBean.olvidoContrasena}" 
						 styleClass="btn btn-success btn-lg btn-block"
						 style="position:relative;top:35px;text-align: center;" />
		 
    	<div style="position: relative;top: 40px;left: 140px">
			<p:ajaxStatus style="width:16px;height:16px;" id="ajaxStatusPanel">
			    <f:facet name="start">
			        <h:graphicImage value="/images/ajax-loader-login.gif" />
			    </f:facet>
			     
			    <f:facet name="complete">
			        <h:outputText value="" />
			    </f:facet>
			</p:ajaxStatus>
		</div>
		<div style="position:relative;top: 35px;">
		 <p:messages id="messages" 
		 			 showDetail="true" 
		 			 autoUpdate="true"
    				 closable="true" />
    	</div>
       </section>

  </div>
</div>

</h:form>

</h:body>
</html>