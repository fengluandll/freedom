<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.2" xmlns="http://xmlns.jcp.org/xml/ns/javaee"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-facesconfig_2_2.xsd">
 <managed-bean>
  <managed-bean-name>loginFilter</managed-bean-name>
  <managed-bean-class>mx.gob.tsjdf.filter.LoginFilter</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 <navigation-rule>
  <from-view-id>/index.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>home</from-outcome>
   <to-view-id>/principal/home.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/principal/home.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>salir</from-outcome>
   <to-view-id>/index.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/solicitudes/capturarSolicitudes.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>nuevaSolicitud</from-outcome>
   <to-view-id>/solicitudes/nuevaSolicitud.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>modificarSolicitud</from-outcome>
   <to-view-id>/solicitudes/modificarSolicitud.xhtml</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/solicitudes/nuevaSolicitud.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>regresar</from-outcome>
   <to-view-id>/solicitudes/capturarSolicitudes.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/solicitudes/modificarSolicitud.xhtml</from-view-id>
  <navigation-case>
   <from-outcome>regresar</from-outcome>
   <to-view-id>/solicitudes/capturarSolicitudes.xhtml</to-view-id>
   <redirect/>
  </navigation-case>
 </navigation-rule>
 <application>
  <message-bundle>mx.gob.tsjdf.properties.application</message-bundle>
  <locale-config>
   <default-locale>la-latin1</default-locale>
  </locale-config>
 </application>
</faces-config>
