	<parameter name="Fecha_Paga" class="java.lang.String">
		<defaultValueExpression><![CDATA[28/02/2015]]></defaultValueExpression>
	</parameter>
	<queryString>
		<![CDATA[SELECT  s.ID_SOLICITUD num_solicitud
      , s.NUM_EMPLEADO
      , s.A_PATERNO
      , s.A_MATERNO
      , s.NOMBRE
      , s.RFC
      , s.JORNADA_ORDINARIA
      , s.FECHA_PAGA
      , s.ID_AREA num_area
      , (SELECT  std_n_work_unitesp
           FROM  std_work_unit wu
           WHERE s.ID_AREA = wu.std_id_work_unit
                 COLLATE DATABASE_DEFAULT) Area_nombre
      , s.ID_ESTATUS_SOLICITUD estatus_solicitud_num
      , es.DESCRIPCION estatus_solicitud_nombre
      -- Inicia las lineas
      , sl.ID_SOLICITUD_HE_ELABORADAS num_line
      , sl.fecha
      , sl.hora_entrada
      , sl.hora_salida
      , sl.num_he_laboradas
      , sl.justificacion
      , (1) num_x
FROM    bdhe.dbo.SOLICITUDES s
      , bdhe.dbo.ESTATUS_SOLICITUD es
      , bdhe.dbo.SOLICITUD_HE_LABORADAS sl
WHERE   s.ID_ESTATUS_SOLICITUD = es.ID_ESTATUS_SOLICITUD
    AND s.ID_SOLICITUD = sl.ID_SOLICITUD
    AND s.FECHA_PAGA = CONVERT(DATETIME, $P{Fecha_Paga}, 103)]]>
	</queryString>
