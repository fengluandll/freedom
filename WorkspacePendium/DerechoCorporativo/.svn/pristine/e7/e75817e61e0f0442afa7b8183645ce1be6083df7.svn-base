package mx.com.televisa.derechocorporativo.util;

public class NumbersUtil {

	

	public static boolean isInteger(String num) {
		
		try {
			Integer.parseInt(num);
			return true;
			
		}catch(Exception ex){
			
			return false;
		}
	}
	
	public static boolean isNumeric(String num) {
		
		try {
			Double.parseDouble(num);
			return true;
			
		}catch(Exception ex){
			
			return false;
		}
	}
}
