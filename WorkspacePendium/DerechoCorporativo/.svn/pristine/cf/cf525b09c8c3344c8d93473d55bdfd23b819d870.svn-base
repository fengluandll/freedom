<%@ page language="java" contentType="text/html; charset=ISO-8859-1"
    pageEncoding="ISO-8859-1"%>
<%@page import="mx.com.televisa.derechocorporativo.data.HQLBeanList"%>
<%@page import="mx.com.televisa.derechocorporativo.model.*"%>
<%@page import="java.util.*"%>
<%@ taglib uri="http://java.sun.com/jsp/jstl/core" prefix="c" %>
<!DOCTYPE html PUBLIC "-//W3C//DTD HTML 4.01 Transitional//EN" "http://www.w3.org/TR/html4/loose.dtd">
<html>
<head>
<meta http-equiv="Content-Type" content="text/html; charset=ISO-8859-1">
<title>Insert title here</title>
</head>
<body>
<%
	HQLBeanList<Seller> h = new HQLBeanList<Seller>();
	List<Seller> sellers = null;  
	try {
		sellers = h.getList("from Seller");
		
	} catch(Exception e) {
		e.printStackTrace();
	} finally {
		h.close();
	}
%>

<table>
	<tr>
		<th>Clave</th>
		<th>Nombre</th>
		<th>Nombre Corto</th>
	</tr>

	<% for(Seller seller : sellers) { %>
	
	<tr>
		<td><%= seller.getSellerId() %></td>
		<td><%= seller.getName() %></td>
		<td><%= seller.getShortName() %></td>
	</tr>
	<% } //End Clients %>
</table>

</body>
</html>