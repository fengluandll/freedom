public class MenuBean implements Serializable{
	
	
	private static final long serialVersionUID = 1L;
	private int      idMenu;
	private int      idMenuElement;
	private int      isEnabled;
	private int      idRol;
	private String	 nomMenu; 
	private int      idMenuElementParent;
	
	/**
	 * Obtiene idMenu
	 * @return
	 */
	public int getIdMenuElementParent() {
		return idMenuElementParent;
	}
	
	/**
	 * AsignaId MenuElemento
	 * @param idMenuElementParent
	 */
	public void setIdMenuElementParent(int idMenuElementParent) {
		this.idMenuElementParent = idMenuElementParent;
	}
	
	/**
	 * Obtiene Id del Menu
	 * @return id del Menu
	 */
	public int getIdMenu() {
		return idMenu;
	}
	
	/**
	 * Asigna IDMenu
	 * @param idMenu
	 */
	public void setIdMenu(int idMenu) {
		this.idMenu = idMenu;
	}
	
	/**
	 * Obtiene IdMenuElement
	 * @return
	 */
	public int getIdMenuElement() {
		return idMenuElement;
	}
	
