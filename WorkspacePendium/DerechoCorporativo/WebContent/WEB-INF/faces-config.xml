<?xml version="1.0" encoding="UTF-8"?>
<faces-config version="2.0" xmlns="http://java.sun.com/xml/ns/javaee"
 xmlns:xi="http://www.w3.org/2001/XInclude"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-facesconfig_2_0.xsd">
 <managed-bean>
  <managed-bean-name>loginBean</managed-bean-name>
  <managed-bean-class>mx.com.televisa.derechocorporativo.modules.login.LoginBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>menuBean</managed-bean-name>
  <managed-bean-class>mx.com.televisa.derechocorporativo.modules.home.MenuBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>applicationBean</managed-bean-name>
  <managed-bean-class>mx.com.televisa.derechocorporativo.bean.ApplicationBean</managed-bean-class>
  <managed-bean-scope>application</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>sessionBean</managed-bean-name>
  <managed-bean-class>mx.com.televisa.derechocorporativo.bean.SessionBean</managed-bean-class>
  <managed-bean-scope>session</managed-bean-scope>
 </managed-bean>
 <managed-bean>
  <managed-bean-name>changePasswordBean</managed-bean-name>
  <managed-bean-class>mx.com.televisa.derechocorporativo.modules.login.ChangePasswordBean</managed-bean-class>
  <managed-bean-scope>request</managed-bean-scope>
  <managed-property>
   <property-name>lblMessage</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>txtOdlPassword</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>txtPassword1</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>txtPassword2</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
  <managed-property>
   <property-name>txtUserName</property-name>
   <property-class>java.lang.String</property-class>
   <value/>
  </managed-property>
 </managed-bean>
 <navigation-rule>
  <display-name>jsp/login/login</display-name>
  <from-view-id>/jsp/login/login.jsp</from-view-id>
  <navigation-case>
   <from-outcome>authorized_access</from-outcome>
   <to-view-id>/jsp/home/main.jsp</to-view-id>
   <redirect/>
  </navigation-case>
  <navigation-case>
   <from-outcome>change_password</from-outcome>
   <to-view-id>/jsp/login/loginChangePassword.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
 <navigation-rule>
  <from-view-id>/jsp/login/loginChangePassword.jsp</from-view-id>
  <navigation-case>
   <from-outcome>authorized_access</from-outcome>
   <to-view-id>/jsp/home/main.jsp</to-view-id>
  </navigation-case>
 </navigation-rule>
</faces-config>
