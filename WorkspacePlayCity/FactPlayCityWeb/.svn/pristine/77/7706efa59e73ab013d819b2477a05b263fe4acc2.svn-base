package mx.com.televisa.playcity.beans;

/*
* Bean de los menús y submenús de la aplicación 
* 
* @author Kaz Consulting / Jesus Argumedo
*
* @version 1.0.0
*
* @date Abril 5, 2019 at 11:00 am
*/

public class SessionMenuBean {
	Integer piIdMenu;
	Integer piIdRol;
	Integer piIdSubmenu;
	Integer piIdRolMenu;
	String  psMenu; 
	String  psMenuUrl;
	String  psSubmenu;
	String  psSubmenuUrl;
	
	public Integer getPiIdMenu() {
		return piIdMenu;
	}
	public void setPiIdMenu(Integer piIdMenu) {
		this.piIdMenu = piIdMenu;
	}
	public Integer getPiIdRol() {
		return piIdRol;
	}
	public void setPiIdRol(Integer piIdRol) {
		this.piIdRol = piIdRol;
	}
	public Integer getPiIdSubmenu() {
		return piIdSubmenu;
	}
	public void setPiIdSubmenu(Integer piIdSubmenu) {
		this.piIdSubmenu = piIdSubmenu;
	}
	public Integer getPiIdRolMenu() {
		return piIdRolMenu;
	}
	public void setPiIdRolMenu(Integer piIdRolMenu) {
		this.piIdRolMenu = piIdRolMenu;
	}
	public String getPsMenu() {
		return psMenu;
	}
	public void setPsMenu(String psMenu) {
		this.psMenu = psMenu;
	}
	public String getPsMenuUrl() {
		return psMenuUrl;
	}
	public void setPsMenuUrl(String psMenuUrl) {
		this.psMenuUrl = psMenuUrl;
	}
	public String getPsSubmenu() {
		return psSubmenu;
	}
	public void setPsSubmenu(String psSubmenu) {
		this.psSubmenu = psSubmenu;
	}
	public String getPsSubmenuUrl() {
		return psSubmenuUrl;
	}
	public void setPsSubmenuUrl(String psSubmenuUrl) {
		this.psSubmenuUrl = psSubmenuUrl;
	}
	public SessionMenuBean() {
		super();
	}
	public SessionMenuBean(Integer tiIdMenu, String tsMenu, String tsMenuUrl) {
		super();
		this.piIdMenu = tiIdMenu;
		this.psMenu = tsMenu;
		this.psMenuUrl = tsMenuUrl;
	}
	public SessionMenuBean(Integer tiIdMenu, Integer tiIdSubmenu, String tsSubmenu,
			String tsSubmenuUrl) {
		super();
		this.piIdMenu = tiIdMenu;
		this.piIdSubmenu = tiIdSubmenu;
		this.psSubmenu = tsSubmenu;
		this.psSubmenuUrl = tsSubmenuUrl;
	}
}
