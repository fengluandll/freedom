<!-- Use this file to deploy some handlers/chains and services      -->
<!-- Two ways to do this:                                           -->
<!--   java org.apache.axis.client.AdminClient deploy.wsdd          -->
<!--      after the axis server is running                          -->
<!-- or                                                             -->
<!--   java org.apache.axis.utils.Admin client|server deploy.wsdd   -->
<!--      from the same directory that the Axis engine runs         -->

<deployment
    xmlns="http://xml.apache.org/axis/wsdd/"
    xmlns:java="http://xml.apache.org/axis/wsdd/providers/java">

  <!-- Services from HolaWSService WSDL service -->

  <service name="HolaWS" provider="java:RPC" style="wrapped" use="literal">
      <parameter name="wsdlTargetNamespace" value="http://start.mx"/>
      <parameter name="wsdlServiceElement" value="HolaWSService"/>
      <parameter name="schemaQualified" value="http://start.mx"/>
      <parameter name="wsdlServicePort" value="HolaWS"/>
      <parameter name="className" value="mx.start.HolaWSSoapBindingImpl"/>
      <parameter name="wsdlPortType" value="HolaWS"/>
      <parameter name="typeMappingVersion" value="1.2"/>
      <operation name="saluda" qname="operNS:saluda" xmlns:operNS="http://start.mx" returnQName="retNS:saludaReturn" xmlns:retNS="http://start.mx" returnType="rtns:string" xmlns:rtns="http://www.w3.org/2001/XMLSchema" soapAction="" >
        <parameter qname="pns:nombre" xmlns:pns="http://start.mx" type="tns:string" xmlns:tns="http://www.w3.org/2001/XMLSchema"/>
      </operation>
      <parameter name="allowedMethods" value="saluda"/>

  </service>
</deployment>
